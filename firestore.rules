rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false;
    }
  }

    match /databases/{database}/documents {
    match /Accepted_Interns/{document=**} {
      allow read: if request.auth != null; // Authenticated users only
    }
  }
}